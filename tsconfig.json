{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": [
			"es2017",
			"dom"
		],
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"preserveConstEnums": true,
		"removeComments": false,
		"noLib": false,
		"declaration": true,
		"sourceMap": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"experimentalDecorators": true,
		"jsx": "react",
		"jsxFactory": "m"
	}
}